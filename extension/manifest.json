{
  "manifest_version": 2,
  

  "name": "History Visualizer",
  "description": "This extension visualizes your history as a graph.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "History Visualizer"
  },

  "permissions": [
    "storage",
    "history",
    /*
    "activeTab",*/
    "https://ajax.googleapis.com/"
  ]
}