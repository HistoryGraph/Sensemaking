{
  "manifest_version": 2,
  

  "name": "History Visualizer",
  "description": "This extension visualizes your history as a graph.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "History Visualizer"
  },
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com/ajax/libs/d3/3.5.5/d3.js; object-src 'self'",
  "permissions": [
    "history",
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://cdnjs.cloudflare.com/ajax/libs/d3/3.5.5/d3.js"
  ]
}