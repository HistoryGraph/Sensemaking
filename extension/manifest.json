{
  "manifest_version": 2,
  

  "name": "History Visualizer",
  "description": "This extension visualizes your history as a graph.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "History Visualizer"
  },

  "content_security_policy": 
    "script-src 'self' https://cdnjs.cloudflare.com/ajax/libs/d3/3.5.5/d3.js https://code.jquery.com/ui/1.11.0/jquery-ui.js https://code.jquery.com/jquery-1.11.0.min.js https://code.jquery.com/ui/1.11.0/themes/smoothness/jquery-ui.css https://bost.ocks.org/mike/fisheye/fisheye.js?0.0.3; object-src 'self'",

  "permissions": [
    "history",
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://cdnjs.cloudflare.com/ajax/libs/d3/3.5.5/d3.js",
    "chrome://favicon/"
  ]


}