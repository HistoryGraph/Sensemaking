{
  "manifest_version": 2,
  

  "name": "History Visualizer",
  "description": "This extension visualizes your history as a graph.",
  "version": "1.0",
/*
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "History Visualizer"
  },
*/
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "background": {
    "scripts": ["https://ajax.googleapis.com/ajax/libs/jquery/1.5.2/jquery.min.js", 
    "https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.12/jquery-ui.min.js",
    "https://apis.google.com/js/plusone.js"]
  },

  "app": { 
    "launch": { 
      "local_path": "index.html" 
    } 
  },

  "default_locale": "en",
  "options_page": "options.html",

  "permissions": [
    "history",
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}